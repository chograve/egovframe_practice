<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="egov.main.dao.EgovMainMapper">
	<select id="selectTblUser" resultType="java.util.HashMap"
		parameterType="java.util.HashMap">
		<![CDATA[	
		select * 
		from tbl_user 
		where user_id = 1
		]]>
	</select>

	<select id="selectLogin" resultType="com.lib.model.UserVO" parameterType="java.util.HashMap">
		<![CDATA[
		select * 
		from tbl_user 
		where user_id = #{id}
		]]>
	</select>
</mapper>